#=== My Aliases ===
alias c='clear'
alias q='exit'
alias ls='ls -a --color=auto'
alias upd='sudo apt-get update'
alias upg='sudo apt-get upgrade'
alias updg='sudo apt-get update && sudo apt-get upgrade -y'
alias adal='sudo vim ~/.bash_aliases'
alias refal='source ~/.zshrc'

alias venv='source venv/bin/activate'
alias vv='virtualenv -p python3 venv'
alias out='deactivate'

alias evalgit='eval "$(ssh-agent -s)" && ssh-add ~/.ssh/git_rsa'

#=== Server ===
alias fddns='eval "$(ssh-agent -s)" && ssh-add ~/.ssh/server_rsa && ssh -p 40408 fantomas@fantomas.ddns.net'
alias aws='ssh -i ~/.ssh/"aws_webserver_frankfurt.pem" ubuntu@ec2-3-120-204-255.eu-central-1.compute.amazonaws.com'
alias dev='eval "$(ssh-agent -s)" && ssh-add ~/.ssh/dev_rsa && ssh developer@192.168.11.132'

#=== Local ===
alias myflask='cd /mnt/c/Users/"Lenovo G550"/Documents/GitHub/flask/microblog/'
alias pip='pip3'
alias py='python3'
alias bot='cd /mnt/c/Users/"Lenovo G550"/Documents/GitHub/bot'
alias desctop='cd /mnt/c/Users/"Lenovo G550"/Desktop'
alias psqlgo='sudo service postgresql start && sudo -u postgres psql postgres'
